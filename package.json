{
  "name": "wordpressium",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.babel.js",
  "scripts": {
    "build": "gulp",
    "watch": "gulp watch",
    "dev": "cross-env NODE_ENV=development npm run watch",
    "prod": "cross-env NODE_ENV=production npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "script": "gulp scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementius/wordpressium.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elementius/wordpressium/issues"
  },
  "homepage": "https://github.com/elementius/wordpressium#readme",
  "engines": {
    "node": ">=8.7.0",
    "npm": ">=5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.17.0",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-imagemin": "^4.0.0",
    "gulp-newer": "^1.3.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sass-bulk-import": "^1.0.1",
    "imagemin-jpegoptim": "^5.1.0",
    "node-notifier": "^5.1.2",
    "postcss-normalize": "^4.0.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-stream": "^1.24.1",
    "run-sequence": "^2.2.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "@maxliving/max-living-style-guide": "^2.3.6",
    "bunnyjs": "^0.14.41",
    "dotenv": "^6.0.0",
    "handlebars": "^4.0.11",
    "sshpk": "^1.14.2"
  },
  "browserslist": [
    "> 2%",
    "last 3 versions",
    "not IE < 10"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    },
    "overrides": [
      {
        "files": "resources/*.js"
      }
    ]
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}

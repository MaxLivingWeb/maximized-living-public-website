{
    "name": "Max Living",
    "description": "Max Living Multisite",
    "keywords": [
        "wordpress"
    ],
    "type": "project",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Jose Castillo",
            "email": "jose.castillo@maxliving.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://wpackagist.org"
          }
    },
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3.3",
        "guzzlehttp/oauth-subscriber": "^0.3.0",
        "johnpbloch/wordpress": "5.2.3",
        "respect/validation": "1.1.29",
        "spomky-labs/jose": "7.1.0",
        "thepixeldeveloper/sitemap": "5.1.3",
        "vlucas/phpdotenv": "2.5.1",
        "wpackagist-plugin/acf-content-analysis-for-yoast-seo":"2.3.0",
        "wpackagist-plugin/classic-editor":"1.5",
        "wpackagist-plugin/contact-form-7":"5.1.4",
        "wpackagist-plugin/contact-form-7-mailchimp-extension":"0.5.02",
        "wpackagist-plugin/contact-form-submissions":"1.6.2",
        "wpackagist-plugin/shortpixel-image-optimiser":"4.14.5",
        "wpackagist-plugin/wordfence":"7.4.0",
        "wpackagist-plugin/wp-external-links":"2.35.0"


    },
    "autoload": {
        "files": [
            "bootstrap/app.php"
        ],
        "psr-4": {
            "MaxLiving\\PeriodicContent\\": "public/plugins/periodic-content",
            "MaxLiving\\ContactForm\\": "public/plugins/contact-form",
            "MaxLiving\\Location\\": "public/plugins/locations-admin",
            "MaxLiving\\AuthPortal\\": "public/plugins/auth-portal",
            "MaxLiving\\PlagiarismTool\\": "public/plugins/plagiarism-tool",
            "MaxLiving\\Sitemaps\\": "public/plugins/sitemaps"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "wordpress-install-dir": "public/app",
        "installer-paths": {
            "public/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "public/themes/{$name}/": [
                "type:wordpress-theme"
            ],
            "public/plugins/required/{$name}/": [
                "type:wordpress-muplugin"
            ]
        }
    }
}
